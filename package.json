{
  "name": "lista-compras-mobile",
  "main": "expo-router/entry",
  "version": "1.3.0",
  "scripts": {
    "prebuild": "expo prebuild",
    "release": "cd android && .\\gradlew assembleRelease && xcopy /Y /Q android\\app\\build\\outputs\\apk\\release\\*.apk _apks\\listadecompras-%npm_package_version%.apk",
    "debug": "cd android && .\\gradlew assembleDebug && xcopy /Y /Q android\\app\\build\\outputs\\apk\\debug\\*.apk _apks\\listadecompras-debug-%npm_package_version%.apk",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "build:android": "eas build -p android --profile preview",
    "build:local": "eas build --platform android --local",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "latest",
    "@expo/vector-icons": "latest",
    "@react-native-async-storage/async-storage": "latest",
    "@react-navigation/native": "latest",
    "expo": "latest",
    "expo-clipboard": "latest",
    "expo-font": "latest",
    "expo-linking": "latest",
    "expo-router": "latest",
    "expo-splash-screen": "latest",
    "expo-status-bar": "latest",
    "expo-system-ui": "latest",
    "expo-web-browser": "latest",
    "nativewind": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-native": "latest",
    "react-native-reanimated": "latest",
    "react-native-safe-area-context": "latest",
    "react-native-screens": "latest",
    "react-native-uuid": "latest",
    "react-native-web": "latest",
    "zustand": "latest",
    "expo-dev-client": "latest"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@rocketseat/eslint-config": "latest",
    "@types/react": "latest",
    "jest": "latest",
    "jest-expo": "latest",
    "react-test-renderer": "latest",
    "tailwindcss": "latest",
    "typescript": "latest",
    "expo-atlas": "latest"
  },
  "private": true
}
